{
  "manifest_version": 1,

  "name": "SearchDirect",
  "description": "This extension shows allows for direct access to page content from google search",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://www.google.com/*"
  ]
  "content_scripts": [
    {
      "matches": ["http://www.google.com/*"],
      "css": ["mystyles.css"],
      "js": ["jquery.js", "googlePage.js"]
    }
  ],
}